{
    "options": {
        "paths": [
        ]
    },
    "dist": {
        "options": {
            "report": "min",
            "strictMath": false,
            "compress": true,
            "cleancss": true
        },
        "files": {
            "<%= dirs.dist %>/styles/main.min.css": "<%= dirs.app %>/styles/main.less"

        }
    },
    "livereload": {
        "options": {
            "strictMath": false,
            "compress": false,
            "cleancss": false
        },
        "files": {
            "<%= dirs.tmp %>/styles/main.css": "<%= dirs.app %>/styles/main.less"

        }
    }

}
