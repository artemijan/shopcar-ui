{
    "dist": {
        "options": {
            "baseUrl": "<%= dirs.app %>/scripts",
            "mainConfigFile": "<%= dirs.app %>/scripts/main.js",
            "paths": {
                "configuration": "empty:",
                "templates": "empty:",
                "permissions": "empty:",
                "require-loader": "bower_components/requirejs/require"
            },
            "uglify2": {
                "warnings": false,
                "mangle": false
            },
            "optimize": "uglify2",
            "name": "main",
            "include": "require-loader",
            "out": "<%= dirs.dist %>/scripts/main.min.js",
            "preserveLicenseComments": false
        }
    }
}
